{
  "name": "surrealdb",
  "version": "0.0.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "npm run build --workspace surrealdb",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "prepare": "npm run build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "npm start --workspace surrealdb -- --no-watch.clearScreen",
    "start:test-app": "npm start --workspace test-app",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "surrealdb",
    "test-app"
  ],
  "dependencies": {
    "surrealdb.js": "^0.7.2"
  }
}
